<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Exercise.
    -->
    <changeSet id="20200702130755-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="move_id"
                                 baseTableName="exercise"
                                 constraintName="fk_exercise_move_id"
                                 referencedColumnNames="id"
                                 referencedTableName="move"/>

        <addForeignKeyConstraint baseColumnNames="workout_id"
                                 baseTableName="exercise"
                                 constraintName="fk_exercise_workout_id"
                                 referencedColumnNames="id"
                                 referencedTableName="workout"/>

    </changeSet>
</databaseChangeLog>
